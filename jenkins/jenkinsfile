pipeline {
    agent any

    stages {
        // express dependency 설치
        stage('Install dependencies') {
            steps {
                sh 'echo shell scrip start'
            }
        }
        // shell script 동작
        stage('Batch Test') {
            steps {
                sh 'npm version patch'
                sh 'node /batch/test.js'
            }
        }
    }
}